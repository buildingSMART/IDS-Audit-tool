<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ids:ids xmlns:ids="http://standards.buildingsmart.org/IDS" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://standards.buildingsmart.org/IDS http://standards.buildingsmart.org/IDS/ids_09.xsd">
	<ids:info>
		<ids:title>random example to show technical capabilities and usage</ids:title>
		<ids:copyright>buildingSMART International Ltd</ids:copyright>
		<ids:date>2021-09-17</ids:date>
	</ids:info>
	<ids:specifications>
		<ids:specification ifcVersion="IFC2X3" name="binder" maxOccurs="unbounded">
			<ids:applicability>
				<ids:entity>
					<ids:name>
						<xs:restriction base="xs:string">
							<!-- no case match -->
							<xs:pattern value="IFCcoVERING"/>
						</xs:restriction>
					</ids:name>
					<ids:predefinedType>
						<ids:simpleValue>CLADDING</ids:simpleValue>
					</ids:predefinedType>
				</ids:entity>
			</ids:applicability>
			<ids:requirements>
				<ids:property maxOccurs="unbounded" datatype="IFCLABEL">
					<ids:propertySet>
						<xs:restriction>
							<xs:pattern value="anas.*"/>
						</xs:restriction>
					</ids:propertySet>
					<ids:name>
						<ids:simpleValue>Anax</ids:simpleValue>
					</ids:name>
					<ids:value>
						<ids:simpleValue>yes</ids:simpleValue>
					</ids:value>
				</ids:property>
			</ids:requirements>
		</ids:specification>
		<ids:specification ifcVersion="IFC2X3" name="binder2" maxOccurs="unbounded">
			<ids:applicability>
				<ids:attribute>
					<ids:name>
						<xs:restriction base="xs:string">
							<!-- invalid valid pattern -->
							<xs:pattern value="[Name"/>
						</xs:restriction>
					</ids:name>
				</ids:attribute>
			</ids:applicability>
			<ids:requirements>
				<ids:attribute>
					<ids:name>
						<xs:restriction>
							<!-- this is a valid pattern that matches-->
							<xs:pattern value="[Nn]ame"/>
						</xs:restriction>
					</ids:name>
				</ids:attribute>
				<ids:attribute>
					<ids:name>
						<xs:restriction>
							<!-- this is a valid pattern that does not match-->
							<xs:pattern value="Invalid[Nn]ame"/>
						</xs:restriction>
					</ids:name>
				</ids:attribute>
				<ids:attribute>
					<ids:name>
						<xs:restriction>
							<!-- this is a valid enumeration  -->
							<xs:enumeration value="Name"/>
							<xs:enumeration value="GlobalId"/>
							<!-- this is not a valid enumeration  -->
							<xs:enumeration value="globalid"/>
						</xs:restriction>
					</ids:name>
				</ids:attribute>
			</ids:requirements>
		</ids:specification>

	</ids:specifications>
</ids:ids>
